                <div id="highscoresEasy" style="text-decoration: underline">Highscores Easy</div>
                <div id="hsrow1easy"> <a id="hsEasy1box">0 Points</a>
                    <div id="appleHsBox1">0x</div>
                </div>
                <div id="hsrow2easy"> <a id="hsEasy2box">0 Points</a>
                    <div id="appleHsBox2">0x</div>
                </div>
                <div id="hsrow3easy"> <a id="hsEasy3box">0 Points</a>
                    <div id="appleHsBox3">0x</div>
                </div>
            </div>
            <div id="scoreboardHardHighscoreBox">
                <div id="highscoresHard">Highscores Hard</div>
                <div id="hsrow1hard"> <a id="hsHard1box">0 Points</a>
