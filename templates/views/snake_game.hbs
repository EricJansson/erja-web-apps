                </div>
                <div id="hsrow2hard"> <a id="hsHard2box">0 Points</a>
                    <div id="fishHsBox2">0x</div>
                </div>
                <div id="hsrow3hard"> <a id="hsHard3box">0 Points</a>
                    <div id="fishHsBox3">0x</div>
                </div>
            </div>




            <button id="endscreenContinue" class="nextLevelAvailable" onclick="quitGame('nextLvl')">Next Level</button>
            <button id="endscreenRestart" onclick="restartGame()">Restart</button>
            <button id="endscreenQuit" onclick="quitGame()">Quit</button>
        </div>

        <div id="countdownBlock">
            Ready?
        </div>
        <div id="blackWhite"></div>

        <div id="foggyOverlay"></div>
