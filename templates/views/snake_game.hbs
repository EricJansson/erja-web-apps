            <u>Paused</u>
            <div id="pauseDecision">
                <button id="pauseResumeBtn" onclick="unpauseGame()">Resume</button>
                <button id="pauseExitBtn" onclick="quickOptionsExit()">Quit</button>
            </div>
            <div id="pauseTextBlock">
