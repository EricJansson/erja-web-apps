                                <div onclick="selectLevel(27, true)" class="smallBall" id="number27">27<div id="levelStar27" class="levelStar"></div></div>
                                <div onclick="selectLevel(28, true)" class="smallBall" id="number28">28<div id="levelStar28" class="levelStar"></div></div>
